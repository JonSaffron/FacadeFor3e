<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net48;net5.0-windows</TargetFrameworks>
    <PackageId>FacadeFor3E</PackageId>
    <Authors>FacadeFor3E</Authors>
    <Product>FacadeFor3E</Product>
    <Version>2.0.0</Version>
    <Description>A set of objects that make using the 3E transaction service a little easier.</Description>
    <Copyright>Copyright © J Saffron Consulting Ltd 2014 - 2022</Copyright>
    <RepositoryUrl>https://github.com/JonSaffron/FacadeFor3e</RepositoryUrl>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
  </PropertyGroup>

  <PropertyGroup>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|netstandard2.0|AnyCPU'">
    <DocumentationFile>C:\Development\NthDegreeTo3e\FacadeFor3e\FacadeFor3e.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="2021.3.0" />
    <PackageReference Include="System.Security.Principal.Windows" Version="5.0.0" />
    <PackageReference Include="System.ServiceModel.Duplex" Version="4.4.*" />
    <PackageReference Include="System.ServiceModel.Http" Version="4.9.0" />
    <PackageReference Include="System.ServiceModel.NetTcp" Version="4.4.*" />
    <PackageReference Include="System.ServiceModel.Primitives" Version="4.9.0" />
    <PackageReference Include="System.ServiceModel.Security" Version="4.4.*" />
  </ItemGroup>

  <ItemGroup>
    <WCFMetadata Include="Connected Services" />
  </ItemGroup>

	<ItemGroup>
		<AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
			<_Parameter1>FacadeFor3e.Tests</_Parameter1>
		</AssemblyAttribute>
	</ItemGroup>
</Project>
